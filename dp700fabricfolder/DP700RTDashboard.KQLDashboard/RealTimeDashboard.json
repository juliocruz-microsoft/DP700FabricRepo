  "queries": [
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "70ada34b-7e31-4910-8570-b348d8dc1585"
      },
      "text": "let temp = 28;\r\nBronzeIoTData\r\n| where temperature > temp\r\n| project-keep messageId, temperature, humidity, PartitionId\r\n| summarize count() by PartitionId\r\n| order by PartitionId asc\r\n| render linechart     ",
      "id": "b9a5f62b-8854-47f4-8350-cb0926413db1",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "70ada34b-7e31-4910-8570-b348d8dc1585"
      },
      "text": "BronzeIoTData",
      "id": "f18a9b83-bb95-440b-a20b-793a099eac0b",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "70ada34b-7e31-4910-8570-b348d8dc1585"
      },
      "text": "BronzeIoTData\n|project-keep messageId, humidity, PartitionId\n|where humidity > 70\n|render linechart ",
      "id": "0befec03-75a9-4f41-817e-9152ddb4adb4",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "70ada34b-7e31-4910-8570-b348d8dc1585"
      },
      "text": "// This KQL query was generated by AI:\n// filter my data by temperature higher than 25, group it by partition and render it with a column chart\n//\nBronzeIoTData\n| where temperature > 25\n| summarize countPartitionId=count() by PartitionId\n| render barchart \n    with (\n        title=\"Count by PartitionId (Temperature > 25)\",\n        ytitle=\"Count\",\n        xtitle=\"PartitionId\",\n        legend=hidden\n    )",
      "id": "f5fd9d8b-f832-4aa8-a804-fd0fc4d2e3f5",
